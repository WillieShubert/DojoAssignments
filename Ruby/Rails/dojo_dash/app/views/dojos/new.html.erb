<div class="container">
  <h1>New Dojo</h1>
  <form  action="/dojos" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <label for="branch">Branch Name:</label><input type="text" name="dojo[branch]">
    <label for="street">Street Name:</label><input type="text" name="dojo[street]">
    <label for="city">City Name:</label><input type="text" name="dojo[city]">
    <label for="state">State Name:</label><input type="text" name="dojo[state]">
    <input type="submit" name="" value="Create Dojo">
  </form>
  <a href="/dojos">Go Back</a>
</div>
