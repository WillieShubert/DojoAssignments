
<h1>Editing <%= @dojo.branch %></h1>
<div class="container">
  <form action="/dojos/<%= @dojo.id %>" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <input type="hidden" name="_method" value="patch">
    <label for="branch">Branch Name:</label><input type="text" name="dojo[branch]" value= "<%= @dojo.branch %>" >
    <label for="street">Street Name:</label><input type="text" name="dojo[street]" value= "<%= @dojo.street %>">
    <label for="city">City Name:</label><input type="text" name="dojo[city]" value= "<%= @dojo.city %>">
    <label for="state">State Name:</label><input type="text" name="dojo[state]" value= "<%= @dojo.state %>">
    <input type="submit" name="" value="Update Dojo">
  </form>
</div>
<a href="/dojos">Home</a> | <a href="/dojos/<%= @dojo.id %>">Show</a>
<form class="" action="/dojos/<%= @dojo.id %>" method="post">
  <input type="hidden" name="_method" value="delete">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="submit" value="Delete">
</form>
